<opc:TypeDictionary xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:tns="http://opcfoundation.org/UA/Test" DefaultByteOrder="LittleEndian" xmlns:opc="http://opcfoundation.org/BinarySchema/" xmlns:ua="http://opcfoundation.org/UA/" TargetNamespace="http://opcfoundation.org/UA/TestNamespace">
    <opc:Import Namespace="http://opcfoundation.org/UA/"/>

    <opc:StructuredType BaseType="ua:ExtensionObject" Name="ObjectWithBitField">
        <opc:Field TypeName="opc:Bit" Name="BitField"/>
        <opc:Field Length="7" TypeName="opc:Bit" Name="Reserved1"/>
        <opc:Field TypeName="opc:Byte" Name="Data"/>
    </opc:StructuredType>

    <opc:StructuredType BaseType="ua:ExtensionObject" Name="ObjectWithNineBits">
        <opc:Field TypeName="opc:Bit" Name="BitField0"/>
        <opc:Field TypeName="opc:Bit" Name="BitField1"/>
        <opc:Field TypeName="opc:Bit" Name="BitField2"/>
        <opc:Field TypeName="opc:Bit" Name="BitField3"/>
        <opc:Field TypeName="opc:Bit" Name="BitField4"/>
        <opc:Field TypeName="opc:Bit" Name="BitField5"/>
        <opc:Field TypeName="opc:Bit" Name="BitField6"/>
        <opc:Field TypeName="opc:Bit" Name="BitField7"/>
        <opc:Field TypeName="opc:Bit" Name="BitField8"/>
        <opc:Field Length="7" TypeName="opc:Bit" Name="Reserved1"/>
        <opc:Field TypeName="opc:Byte" Name="Data" SwitchField="BitField8"/>
    </opc:StructuredType>

    <opc:StructuredType BaseType="ua:ExtensionObject" Name="ObjectWithBitsOptionalFields">
        <opc:Field TypeName="opc:Bit" Name="OptionOne"/>
        <opc:Field TypeName="opc:Bit" Name="OptionTwo"/>
        <opc:Field Length="6" TypeName="opc:Bit" Name="Reserved1"/>
        <opc:Field TypeName="opc:Byte" Name="OptionalByteOne" SwitchField="OptionOne"/>
        <opc:Field TypeName="opc:Byte" Name="OptionalByteTwo" SwitchField="OptionTwo"/>
    </opc:StructuredType>

    <opc:StructuredType BaseType="ua:ExtensionObject" Name="SwitchValueObject">
        <opc:Field TypeName="opc:Bit" Name="OptionOne"/>
        <opc:Field TypeName="opc:Bit" Name="OptionTwo"/>
        <opc:Field Length="6" TypeName="opc:Bit" Name="Reserved1"/>
        <opc:Field TypeName="opc:Byte" Name="NotOne" SwitchField="OptionOne" SwitchValue="0"/>
        <opc:Field TypeName="opc:Byte" Name="NotTwo" SwitchField="OptionTwo" SwitchValue="0"/>
        <opc:Field TypeName="opc:Byte" Name="YesTwo" SwitchField="OptionTwo" SwitchValue="1"/>
        <opc:Field TypeName="opc:Byte" Name="AlsoNotOne" SwitchField="OptionOne" SwitchValue="0"/>
    </opc:StructuredType>

</opc:TypeDictionary>
